{
  "name": "playmotiv.react.learning",
  "version": "1.0.0",
  "description": "Ejercicios de partida para aprender react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaldeera/playmotiv.react.learning.git"
  },
  "author": "Alberto Moral, Dani Servian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kaldeera/playmotiv.react.learning/issues"
  },
  "homepage": "https://github.com/Kaldeera/playmotiv.react.learning#readme",
  "dependencies": {
    "@wordpress/components": "^8.0.0",
    "antd": "^3.20.7",
    "interactjs": "^1.5.4",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-calendar-timeline": "^0.26.1",
    "react-dom": "^16.8.6",
    "react-html-parser": "^2.0.2",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.1.0",
    "react-sortable-tree": "^2.6.2",
    "react-sortable-tree-theme-minimal": "0.0.14",
    "react-spring": "^8.0.27",
    "react-tabulator": "^0.9.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "serve": "^11.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "scripts": {
    "react-hooks:build": "cd react-hooks && webpack --env=react-hooks",
    "react-hooks:dev": "cd react-hooks && webpack-dev-server --progress --hot --open --env=react-hooks",
    "react-classes:build": "cd react-classes && webpack --env=react-classes",
    "react-classes:dev": "cd react-classes && webpack-dev-server --progress --hot --open --env=react-classes",
    "react-functions-props:build": "cd react-functions-props && webpack --env=react-functions-props",
    "react-functions-props:dev": "cd react-functions-props && webpack-dev-server --progress --hot --open --env=react-functions-props",
    "react-functions:build": "cd react-functions && webpack --env=react-functions",
    "react-functions:dev": "cd react-functions && webpack-dev-server --progress --hot --open --env=react-functions",
    "react-vanilla": "cd react-vanilla && serve",
    "react-redux:dev": "cd react-redux && webpack-dev-server --progress --hot --open --quiet --env=react-redux",
    "react-redux-grid:dev": "cd react-redux-grid && webpack-dev-server --progress --hot --open --quiet --env=react-redux-grid",
    "react-desdecero:dev": "cd react-desdecero && webpack-dev-server --progress --hot --open --quiet --env=react-desdecero",
    "react-tree-classes:dev": "cd react-tree-classes && webpack-dev-server --progress --hot --open --quiet --env=react-tree-classes",
    "react-tree-functions:dev": "cd react-tree-functions && webpack-dev-server --progress --hot --open --quiet --env=react-tree-functions",
    "react-tree-antdesign:dev": "cd react-tree-antdesign && webpack-dev-server --progress --hot --open --quiet --env=react-tree-antdesign",
    "react-calendar-timeline:dev": "cd react-calendar-timeline && webpack-dev-server --progress --hot --open --quiet --env=react-calendar-timeline",
    "react-context:dev": "cd react-context && webpack-dev-server --progress --hot --open --quiet --env=react-context",
    "react-html-parser:dev": "cd react-html-parser && webpack-dev-server --progress --hot --open --quiet --env=react-html-parser",
    "react-redux-usecontext:dev": "cd react-redux-usecontext && webpack-dev-server --progress --hot --open --quiet --env=react-redux-usecontext",
    "react-lazy-load:dev": "cd react-lazy-load && webpack-dev-server --progress --hot --open --quiet --env=react-lazy-load",
    "react-lazy-load:build": "cd react-lazy-load && webpack --env=react-lazy-load",
    "react-spring:dev": "cd react-spring && webpack-dev-server --progress --hot --open --quiet --env=react-spring",
    "react-redux-oveload:dev": "cd react-redux-overload && webpack-dev-server --progress --hot --open --quiet --env=react-redux-overload"
  }
}
